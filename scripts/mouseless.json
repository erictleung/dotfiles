{
    "plugin": "mouseless",
    "browser": "firefox",
    "blob_characters": ";asdjfir",
    "auto_submit_timer": 0,
    "blacklisted_urls": [
        "https://calendar.google.com/*",
        "https://duckduckgo.com/*",
        "http://localhost:8888/*",
        "https://docs.google.com/presentation/*",
        "https://www.nytimes.com/games/wordle/index.html",
        "https://mail.google.com/*",
        "https://youtube.com/*"
    ],
    "scroll": {
        "up": "k",
        "down": "j",
        "up_fast": "<Shift>_",
        "down_fast": "<Shift>+"
    },
    "click": "Enter",
    "open_in_new_tab": "<Shift>Enter",
    "save_url_to_clipboard": "<Control>Enter",
    "focus": "Tab",
    "blobs": {
        "show": "/",
        "hide": "Escape",
        "backspace": "Backspace"
    },
    "deselect_current_element": "Escape",
    "tab": {
        "switch_left": "h",
        "switch_right": "l",
        "move_left": "<Shift>H",
        "movie_right": "<Shift>L"
    },
    "go": {
        "back": "<Shift>B",
        "forward": "<Shift>F"
    }
}
