__conda_setup="$('/home/leunge/miniconda/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/leunge/miniconda/etc/profile.d/conda.sh" ]; then
        . "/home/leunge/miniconda/etc/profile.d/conda.sh"
    else
        export PATH="/home/leunge/miniconda/bin:$PATH"
    fi
fi
unset __conda_setup
